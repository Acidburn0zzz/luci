[
  {
    "cmd": [
      "luci-auth",
      "token",
      "-lifetime",
      "3m"
    ],
    "infra_step": true,
    "name": "get access token for default account",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "step returned non-zero exit code: 1",
      "@@@STEP_EXCEPTION@@@"
    ]
  },
  {
    "failure": {
      "exception": {
        "traceback": [
          "<omitted by recipe engine>"
        ]
      },
      "humanReason": "Infra Failure: Step('get access token for default account') returned 1"
    },
    "name": "$result"
  }
]