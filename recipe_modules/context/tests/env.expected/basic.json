[
  {
    "cmd": [
      "echo",
      "hello"
    ],
    "name": "no env"
  },
  {
    "cmd": [
      "echo",
      "hello"
    ],
    "env": {
      "SOMETHING": "1"
    },
    "name": "with env"
  },
  {
    "cmd": [
      "echo",
      "hello"
    ],
    "env": {
      "SOMETHING": "1",
      "SOMETHING_ELSE": "0"
    },
    "name": "with 2 envs"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "name": "default",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": null
    },
    "name": "drop",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": "bar"
    },
    "name": "env step",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": "foo:<RECIPE_ENGINE_CONTEXT_TEST>:bar"
    },
    "name": "env step augmented",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": "foo:<RECIPE_ENGINE_CONTEXT_TEST>:bar"
    },
    "env_prefixes": {
      "RECIPE_ENGINE_CONTEXT_TEST": [
        "[START_DIR]/pants",
        "[START_DIR]/shirt"
      ]
    },
    "name": "env step with prefix",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env_prefixes": {
      "RECIPE_ENGINE_CONTEXT_TEST": [
        "[START_DIR]/shirt"
      ]
    },
    "name": "env with default value",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": "foo"
    },
    "env_prefixes": {
      "RECIPE_ENGINE_CONTEXT_TEST": [
        "[START_DIR]/shirt"
      ]
    },
    "name": "env with override value",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": null
    },
    "env_prefixes": {
      "RECIPE_ENGINE_CONTEXT_TEST": [
        "[START_DIR]/shirt"
      ]
    },
    "name": "env with cleared value",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "bash",
      "-c",
      "echo -n $RECIPE_ENGINE_CONTEXT_TEST"
    ],
    "env": {
      "RECIPE_ENGINE_CONTEXT_TEST": ""
    },
    "env_prefixes": {
      "RECIPE_ENGINE_CONTEXT_TEST": [
        "[START_DIR]/shirt"
      ]
    },
    "name": "env with empty value",
    "stdout": "/path/to/tmp/"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
