      "@@@STEP_LOG_LINE@json.output[output]@{@@@",
      "@@@STEP_LOG_LINE@json.output[output]@  \"is_json\": true@@@",
      "@@@STEP_LOG_LINE@json.output[output]@}@@@",
      "@@@STEP_LOG_END@json.output[output]@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "http://httpstat.us/500",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "/path/to/tmp/",
      "--transient-retry",
      "4"
    ],
    "name": "error"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "http://httpstat.us/500",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "/path/to/tmp/",
      "--transient-retry",
      "0"
