[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
      "--url",
      "https://chromium.org",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "[START_DIR]/download.bin",
      "--headers-json",
      "{\"Authorization\": \"thing\"}"
    ],
    "name": "GET https://chromium.org"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]/resources/pycurl.py",
