[
  {
    "cmd": [
      "touch",
      "[CLEANUP]/isolated-example_tmp_1/a"
    ],
    "name": "touch a"
  },
  {
    "cmd": [
      "touch",
      "[CLEANUP]/isolated-example_tmp_1/b"
    ],
    "name": "touch b"
  },
  {
    "cmd": [
      "touch",
      "[CLEANUP]/isolated-example_tmp_1/c"
    ],
    "name": "touch c"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[CLEANUP]/isolated-example_tmp_1/sub/dir"
    ],
    "infra_step": true,
    "name": "mkdirs"
  },
  {
    "cmd": [
      "touch",
      "[CLEANUP]/isolated-example_tmp_1/sub/dir/d"
    ],
    "name": "touch d"
  },
  {
    "cmd": [],
    "name": "ensure isolated"
  },
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[CACHE]/isolated_client",
      "-ensure-file",
      "infra/tools/luci/isolated/${platform} release",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "infra_step": true,
    "name": "ensure isolated.ensure_installed",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"instance_id\": \"resolved-instance_id-of-release---------\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"package\": \"infra/tools/luci/isolated/resolved-platform\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[CACHE]/isolated_client/isolated",
      "archive",
      "-isolate-server",
      "https://example.isolateserver.appspot.com",
      "-namespace",
      "default-gzip",
      "-dump-hash",
      "/path/to/tmp/",
      "-files",
      "[CLEANUP]/isolated-example_tmp_1:a",
      "-files",
      "[CLEANUP]/isolated-example_tmp_1:b",
      "-files",
      "[CLEANUP]/isolated-example_tmp_1:c",
      "-dirs",
      "[CLEANUP]/isolated-example_tmp_1:sub/dir"
    ],
    "name": "archiving"
  },
  {
    "cmd": [
      "[CACHE]/isolated_client/isolated",
      "archive",
      "-isolate-server",
      "other-isolateserver.appspot.com",
      "-namespace",
      "default-gzip",
      "-dump-hash",
      "/path/to/tmp/",
      "-dirs",
      "[CLEANUP]:isolated-example_tmp_1"
    ],
    "name": "archiving root directory elsewhere"
  },
  {
    "cmd": [
      "[CACHE]/isolated_client/isolated",
      "download",
      "-isolate-server",
      "https://example.isolateserver.appspot.com",
      "-isolated",
      "[dummy hash]",
      "-output-dir",
      "[CLEANUP]/first"
    ],
    "name": "download with first hash"
  },
  {
    "cmd": [
      "[CACHE]/isolated_client/isolated",
      "download",
      "-isolate-server",
      "other-isolateserver.appspot.com",
      "-isolated",
      "[dummy hash]",
      "-output-dir",
      "[CLEANUP]/second"
    ],
    "name": "download with second hash"
  },
  {
    "cmd": [],
    "env_prefixes": {
      "PATH": [
        "[CACHE]/isolated_client"
      ]
    },
    "name": "some step with isolated in path"
  },
  {
    "jsonResult": null,
    "name": "$result"
  }
]